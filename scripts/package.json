{
  "name": "rabat-mvp-data-import",
  "version": "1.0.0",
  "description": "Data import script for Rabat MVP project",
  "main": "import-data.js",
  "scripts": {
    "import": "node import-data.js",
    "import:projects": "node -e \"require('./import-data.js').importProjects()\"",
